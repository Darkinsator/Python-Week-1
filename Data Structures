#Lists
courses = ['Maths', 'Physics', 'English', 'CompScie']
courses.insert(0, 'Art')
print(courses)

#Sets
thisfruit = {"apple", "banana", "cherry"}
thisvegetables= {"carrot", "potato", "spinach"}
print(thisfruit.union(thisvegetables))

#Tuples
thistuple = ("apple", "banana", "cherry")
print(thistuple)    

#Dictionaries
thisdict = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
print(thisdict)

